{
  "version": 2,
  "name": "arc-grid-server",
  "builds": [
    {
      "src": "api/check-access.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/check-access",
      "destination": "/api/check-access.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/check-access.js"
    }
  ]
}